<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
	<PackageId>MatinDeWet.Pager</PackageId>
	<Version>1.1.1</Version>
	<Authors>MatinDeWet</Authors>
	<Product>Pager</Product>
	<Description>Pager allows you to create paged results while making use of entity framework core</Description>
	<RepositoryUrl>https://github.com/MatinDeWet/Pager</RepositoryUrl>
	<PackageProjectUrl>https://github.com/MatinDeWet/Pager</PackageProjectUrl>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
	<Title>Pager</Title>
  </PropertyGroup>

	<ItemGroup>
		<None Include="..\LICENSE.txt">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.6" />
  </ItemGroup>

</Project>
